{
    "pluginName": "变量快速翻译命名插件",
    "version": "0.1.8",
    "description": "使用翻译进行变量格式化命名.",
    "author": "乔安",
    "homepage": "https://www.motouguai.com",
    "preload": "preload.js",
    "logo": "logo.png",
    "features": [
        {
            "code": "big_hump",
            "explain": "转换内容为大驼峰命名格式",
            "cmds": [
                "大驼峰", "dt"
            ]
        },
        {
            "code": "small_hump",
            "explain": "转换内容为小驼峰命名格式",
            "cmds": [
                "小驼峰", "xt"
            ]
        },
        {
            "code": "underline",
            "explain": "转换内容为下划线命名格式",
            "cmds": [
                "下划线", "xh"
            ]
        },
        {
            "code": "horizontal_line",
            "explain": "转换内容为横线命名格式",
            "cmds": [
                "横线", "hx"
            ]
        },
        {
            "code": "constant",
            "explain": "转换内容为常量命名格式",
            "cmds": [
                "常量", "cl"
            ]
        },
        {
            "code": "whether_to_shut_down_automatical",
            "explain": "切换变量快速翻译命名插件是否自动关闭设置",
            "cmds": [
                "变量快速翻译命名插件是否自动关闭"
            ]
        },
        {
            "code": "any_text_match",
            "explain": "切换变量快速翻译命名插件是否任意文本匹配插件",
            "cmds": [
                "变量快速翻译命名插件是否任意文本匹配插件(可配合超级面板)"
            ]
        }
    ]
}